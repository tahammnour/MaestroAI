# MaestroAI Environment Variables
# Copy this file to .env and fill in your values

# Azure OpenAI Service
OPENAI_ENDPOINT=https://your-openai-resource.openai.azure.com/
OPENAI_API_KEY=your-openai-api-key
OPENAI_DEPLOYMENT_NAME=gpt-4
OPENAI_API_VERSION=2024-02-15-preview

# Azure Cosmos DB
COSMOS_ENDPOINT=https://your-cosmos-account.documents.azure.com:443/
COSMOS_KEY=your-cosmos-primary-key
COSMOS_DATABASE=maestroai-db

# Azure Cognitive Search
SEARCH_ENDPOINT=https://your-search-service.search.windows.net
SEARCH_KEY=your-search-admin-key
SEARCH_INDEX_POLICIES=hr-policies-index
SEARCH_INDEX_FAQ=faq-index

# Azure Storage
STORAGE_ACCOUNT_NAME=your-storage-account
STORAGE_CONNECTION_STRING=your-storage-connection-string

# Azure Key Vault
KEY_VAULT_NAME=your-key-vault-name

# Azure Function App
FUNCTION_APP_NAME=maestroai-functions
FUNCTION_APP_URL=https://maestroai-functions.azurewebsites.net

# Azure API Management
APIM_NAME=maestroai-apim
APIM_URL=https://maestroai-apim.azure-api.net

# Application Insights
APPINSIGHTS_CONNECTION_STRING=your-appinsights-connection-string

# Logic App
LOGIC_APP_URL=https://your-logic-app-url.azurewebsites.net

# Teams Bot (if using)
TEAMS_BOT_ID=your-teams-bot-id
TEAMS_BOT_PASSWORD=your-teams-bot-password

# Security
AAD_TENANT_ID=your-tenant-id
AAD_CLIENT_ID=your-client-id
AAD_CLIENT_SECRET=your-client-secret

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO
MAX_RETRIES=3
TIMEOUT_SECONDS=30

